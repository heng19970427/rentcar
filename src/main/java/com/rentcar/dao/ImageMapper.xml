<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rentcar.dao.ImageMapper">
    <select id="queryCarImgPath" parameterType="int" resultType="Image">
        SELECT * FROM image WHERE imageId=#{v}
    </select>

    <insert id="insertImage" useGeneratedKeys="true" keyColumn="imageId" keyProperty="imageId" parameterType="Image">
        INSERT into image
        VALUES (
          null,
          #{path},
          #{title}
        )
    </insert>
</mapper>